<h1>Anthony Lee</h1><br />

<table>
    <thead>
        <tr>
            <th>Project Name</th>
            <th>Description</th>
            <th>URL</th>
        </tr>
    </thead>
    <tbody>
        <% PROJECTS.each do |project, data| %>
            <tr>
                <td><%= project %></td>
                <td><%= data['description'] %></td>
                <td><%= link_to data['url'] %></td>
            </tr>
        <% end %>
    </tbody>
</table>
